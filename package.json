{
  "name": "mini-twitch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --typecheck",
    "cy:test": "cypress open --env NODE_ENV=test",
    "cy:dev": "cypress open --env NODE_ENV=development",
    "cy:run": "cypress open --env NODE_ENV=test",
    "dotenv": "dotenv -e .env.development.local",
    "prisma": "dotenv -e .env.development.local prisma",
    "prisma:validate": "dotenv -e .env.development.local prisma validate",
    "prisma:push": "dotenv -e .env.development.local prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "dotenv-cli": "^7.3.0",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10",
    "cypress": "^13.3.0",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "postcss": "^8",
    "prisma": "^5.7.0",
    "tailwindcss": "^3",
    "typescript": "^5",
    "vitest": "^1.0.1"
  }
}
